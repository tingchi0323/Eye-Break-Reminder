  "manifest_version": 3,
  "name": "Eye Break Reminder",
  "version": "1.0",
  "description": "定時提醒你讓眼睛休息的小工具",
  "permissions": ["notifications", "alarms"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "48": "icon.jpg"
  },
  "action":{
    "default_popup":"popup.html"
    }
}